						<item value="1">Cross Above</item>
						<item value="2">Cross Below</item>
					</combo>
					<combo postlabel="" name="m_mainIndicator" is_control="true" selected_index="1">
						<item value="1">RH Line</item>
						<item value="2">RL Line</item>
					</combo>
					<crlf is_control="true"/>
