<?xml version="1.0" encoding="UTF-8"?>
<stchart_systrade_file is_trade="true"><signal_conflist><buy isroot="yes" operator="and"><signal type="indicator" id="autotrend" title="Auto Trend Line" operator="and">
				<sig_methods>
					<itm>cross_price_autotrend</itm>
				</sig_methods>
				<signal_method id="cross_price_autotrend" title="Price Crossover">
